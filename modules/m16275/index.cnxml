                  </m:mfenced>
                </m:mrow>
              </m:mtd>
              <m:mtd>
                <m:mo>=</m:mo>
              </m:mtd>
              <m:mtd columnalign="left">
                <m:mrow>
                  <m:munderover>
                    <m:mo>∑</m:mo>
                    <m:mrow>
                      <m:mi>i</m:mi>
                      <m:mo>=</m:mo>
                      <m:mn>1</m:mn>
                    </m:mrow>
                    <m:mi>n</m:mi>
                  </m:munderover>
                  <m:mspace width="0.166667em"/>
                  <m:mo>-</m:mo>
                  <m:mn>2</m:mn>
                  <m:mo form="prefix">log</m:mo>
                  <m:mi>A</m:mi>
                  <m:mfenced separators="" open="(" close=")">
                    <m:msub>
                      <m:mi>p</m:mi>
                      <m:mrow>
                        <m:mover accent="true">
                          <m:msub>
                            <m:mi>f</m:mi>
