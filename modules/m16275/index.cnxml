                      <m:mo>·</m:mo>
                      <m:mfrac>
                        <m:msup>
                          <m:mi>e</m:mi>
                          <m:mrow>
                            <m:mo>-</m:mo>
                            <m:mi>c</m:mi>
                            <m:mo>(</m:mo>
                            <m:mover accent="true">
                              <m:msub>
                                <m:mi>f</m:mi>
                                <m:mi>n</m:mi>
                              </m:msub>
                              <m:mo>^</m:mo>
                            </m:mover>
                            <m:mo>)</m:mo>
                            <m:mo form="prefix">log</m:mo>
                            <m:mn>2</m:mn>
                          </m:mrow>
                        </m:msup>
                        <m:mrow>
                          <m:mo>∫</m:mo>
                          <m:msqrt>
                            <m:mrow>
                              <m:msub>
                                <m:mi>p</m:mi>
                                <m:mover accent="true">
                                  <m:msub>
                                    <m:mi>f</m:mi>
                                    <m:mi>n</m:mi>
                                  </m:msub>
                                  <m:mo>^</m:mo>
                                </m:mover>
                              </m:msub>
                              <m:mrow>
                                <m:mo>(</m:mo>
                                <m:mi>y</m:mi>
                                <m:mo>)</m:mo>
                              </m:mrow>
                              <m:mo>·</m:mo>
                              <m:msub>
                                <m:mi>p</m:mi>
                                <m:msup>
                                  <m:mi>f</m:mi>
                                  <m:mo>*</m:mo>
                                </m:msup>
                              </m:msub>
                              <m:mrow>
                                <m:mo>(</m:mo>
                                <m:mi>y</m:mi>
                                <m:mo>)</m:mo>
                              </m:mrow>
                            </m:mrow>
                          </m:msqrt>
                          <m:mspace width="0.166667em"/>
                          <m:mi mathvariant="normal">d</m:mi>
                          <m:mi>y</m:mi>
                        </m:mrow>
                      </m:mfrac>
                    </m:mfenced>
                  </m:mfenced>
                </m:mrow>
              </m:mtd>
              <m:mtd>
                <m:mo>≤</m:mo>
              </m:mtd>
              <m:mtd columnalign="left">
                <m:mrow>
                  <m:mn>2</m:mn>
                  <m:mo form="prefix">log</m:mo>
                  <m:mfenced separators="" open="(" close=")">
                    <m:mi>E</m:mi>
                    <m:mfenced separators="" open="[" close="]">
                      <m:munder>
                        <m:mo>∑</m:mo>
                        <m:mrow>
                          <m:mi>f</m:mi>
                          <m:mo>∈</m:mo>
                          <m:mi mathvariant="script">F</m:mi>
                        </m:mrow>
                      </m:munder>
                      <m:msup>
                        <m:mi>e</m:mi>
                        <m:mrow>
                          <m:mo>-</m:mo>
                          <m:mi>c</m:mi>
                          <m:mo>(</m:mo>
                          <m:mi>f</m:mi>
                          <m:mo>)</m:mo>
                          <m:mo form="prefix">log</m:mo>
                          <m:mn>2</m:mn>
                        </m:mrow>
                      </m:msup>
                      <m:mo>·</m:mo>
                      <m:mfrac>
                        <m:msqrt>
                          <m:mfrac>
                            <m:mrow>
                              <m:msub>
                                <m:mi>p</m:mi>
                                <m:mi>f</m:mi>
                              </m:msub>
                              <m:mrow>
                                <m:mo>(</m:mo>
                                <m:mi>Y</m:mi>
                                <m:mo>)</m:mo>
                              </m:mrow>
                            </m:mrow>
                            <m:mrow>
                              <m:msub>
                                <m:mi>p</m:mi>
                                <m:msup>
                                  <m:mi>f</m:mi>
                                  <m:mo>*</m:mo>
                                </m:msup>
                              </m:msub>
                              <m:mrow>
                                <m:mo>(</m:mo>
                                <m:mi>Y</m:mi>
                                <m:mo>)</m:mo>
                              </m:mrow>
                            </m:mrow>
                          </m:mfrac>
                        </m:msqrt>
                        <m:mrow>
                          <m:mo>∫</m:mo>
                          <m:msqrt>
                            <m:mrow>
                              <m:msub>
                                <m:mi>p</m:mi>
                                <m:mi>f</m:mi>
                              </m:msub>
                              <m:mrow>
                                <m:mo>(</m:mo>
                                <m:mi>y</m:mi>
                                <m:mo>)</m:mo>
                              </m:mrow>
                              <m:mo>·</m:mo>
                              <m:msub>
                                <m:mi>p</m:mi>
                                <m:msup>
                                  <m:mi>f</m:mi>
                                  <m:mo>*</m:mo>
                                </m:msup>
                              </m:msub>
                              <m:mrow>
                                <m:mo>(</m:mo>
                                <m:mi>y</m:mi>
                                <m:mo>)</m:mo>
                              </m:mrow>
                            </m:mrow>
                          </m:msqrt>
                          <m:mspace width="0.166667em"/>
                          <m:mi mathvariant="normal">d</m:mi>
                          <m:mi>y</m:mi>
                        </m:mrow>
                      </m:mfrac>
                    </m:mfenced>
                  </m:mfenced>
                </m:mrow>
              </m:mtd>
            </m:mtr>
            <m:mtr>
              <m:mtd/>
              <m:mtd>
                <m:mo>=</m:mo>
              </m:mtd>
              <m:mtd columnalign="left">
                <m:mrow>
                  <m:mn>2</m:mn>
                  <m:mo form="prefix">log</m:mo>
                  <m:mfenced separators="" open="(" close=")">
                    <m:munder>
                      <m:mo>∑</m:mo>
                      <m:mrow>
                        <m:mi>f</m:mi>
                        <m:mo>∈</m:mo>
                        <m:mi mathvariant="script">F</m:mi>
                      </m:mrow>
                    </m:munder>
                    <m:msup>
                      <m:mn>2</m:mn>
                      <m:mrow>
                        <m:mo>-</m:mo>
                        <m:mi>c</m:mi>
                        <m:mo>(</m:mo>
                        <m:mi>f</m:mi>
                        <m:mo>)</m:mo>
                      </m:mrow>
                    </m:msup>
                    <m:mfrac>
                      <m:mrow>
                        <m:mi>E</m:mi>
                        <m:mfenced open="[" close="]">
                          <m:msqrt>
                            <m:mfrac>
                              <m:mrow>
                                <m:msub>
                                  <m:mi>p</m:mi>
                                  <m:mi>f</m:mi>
                                </m:msub>
                                <m:mrow>
                                  <m:mo>(</m:mo>
                                  <m:mi>Y</m:mi>
                                  <m:mo>)</m:mo>
                                </m:mrow>
                              </m:mrow>
                              <m:mrow>
                                <m:msub>
                                  <m:mi>p</m:mi>
                                  <m:msup>
                                    <m:mi>f</m:mi>
                                    <m:mo>*</m:mo>
                                  </m:msup>
                                </m:msub>
                                <m:mrow>
                                  <m:mo>(</m:mo>
                                  <m:mi>Y</m:mi>
                                  <m:mo>)</m:mo>
                                </m:mrow>
                              </m:mrow>
                            </m:mfrac>
                          </m:msqrt>
                        </m:mfenced>
                      </m:mrow>
                      <m:mrow>
                        <m:mo>∫</m:mo>
                        <m:msqrt>
                          <m:mrow>
                            <m:msub>
                              <m:mi>p</m:mi>
                              <m:mi>f</m:mi>
                            </m:msub>
                            <m:mrow>
                              <m:mo>(</m:mo>
                              <m:mi>y</m:mi>
                              <m:mo>)</m:mo>
                            </m:mrow>
                            <m:mo>·</m:mo>
                            <m:msub>
                              <m:mi>p</m:mi>
                              <m:msup>
                                <m:mi>f</m:mi>
                                <m:mo>*</m:mo>
                              </m:msup>
                            </m:msub>
                            <m:mrow>
                              <m:mo>(</m:mo>
                              <m:mi>y</m:mi>
                              <m:mo>)</m:mo>
                            </m:mrow>
                          </m:mrow>
                        </m:msqrt>
                        <m:mspace width="0.166667em"/>
                        <m:mi mathvariant="normal">d</m:mi>
                        <m:mi>y</m:mi>
                      </m:mrow>
                    </m:mfrac>
                  </m:mfenced>
                </m:mrow>
              </m:mtd>
            </m:mtr>
            <m:mtr>
              <m:mtd/>
              <m:mtd>
                <m:mo>=</m:mo>
              </m:mtd>
              <m:mtd columnalign="left">
                <m:mrow>
                  <m:mn>2</m:mn>
                  <m:mo form="prefix">log</m:mo>
                  <m:mfenced separators="" open="(" close=")">
                    <m:munder>
                      <m:mo>∑</m:mo>
                      <m:mrow>
                        <m:mi>f</m:mi>
                        <m:mo>∈</m:mo>
                        <m:mi mathvariant="script">F</m:mi>
                      </m:mrow>
                    </m:munder>
                    <m:msup>
                      <m:mn>2</m:mn>
                      <m:mrow>
                        <m:mo>-</m:mo>
                        <m:mi>c</m:mi>
                        <m:mo>(</m:mo>
                        <m:mi>f</m:mi>
                        <m:mo>)</m:mo>
                      </m:mrow>
                    </m:msup>
                  </m:mfenced>
                </m:mrow>
              </m:mtd>
            </m:mtr>
            <m:mtr>
              <m:mtd/>
              <m:mtd>
                <m:mo>≤</m:mo>
              </m:mtd>
              <m:mtd columnalign="left">
                <m:mrow>
                  <m:mn>0</m:mn>
                  <m:mo>.</m:mo>
                </m:mrow>
              </m:mtd>
            </m:mtr>
          </m:mtable>
        </m:math>
      </equation>
      <para id="id2269659">where the last two lines come from</para>
      <equation id="id2269665">
        <m:math mode="display">
          <m:mrow>
            <m:mi>E</m:mi>
            <m:mfenced open="[" close="]">
              <m:msqrt>
                <m:mfrac>
                  <m:mrow>
                    <m:msub>
                      <m:mi>p</m:mi>
                      <m:mi>f</m:mi>
                    </m:msub>
                    <m:mrow>
                      <m:mo>(</m:mo>
                      <m:mi>Y</m:mi>
                      <m:mo>)</m:mo>
                    </m:mrow>
                  </m:mrow>
                  <m:mrow>
                    <m:msub>
                      <m:mi>p</m:mi>
                      <m:msup>
                        <m:mi>f</m:mi>
                        <m:mo>*</m:mo>
                      </m:msup>
                    </m:msub>
                    <m:mrow>
                      <m:mo>(</m:mo>
                      <m:mi>Y</m:mi>
                      <m:mo>)</m:mo>
                    </m:mrow>
                  </m:mrow>
                </m:mfrac>
              </m:msqrt>
            </m:mfenced>
            <m:mo>=</m:mo>
            <m:mo>∫</m:mo>
            <m:msqrt>
              <m:mfrac>
                <m:mrow>
                  <m:msub>
                    <m:mi>p</m:mi>
                    <m:mi>f</m:mi>
                  </m:msub>
                  <m:mrow>
                    <m:mo>(</m:mo>
                    <m:mi>y</m:mi>
                    <m:mo>)</m:mo>
                  </m:mrow>
                </m:mrow>
                <m:mrow>
                  <m:msub>
                    <m:mi>p</m:mi>
                    <m:msup>
                      <m:mi>f</m:mi>
                      <m:mo>*</m:mo>
                    </m:msup>
                  </m:msub>
                  <m:mrow>
                    <m:mo>(</m:mo>
                    <m:mi>y</m:mi>
                    <m:mo>)</m:mo>
                  </m:mrow>
                </m:mrow>
              </m:mfrac>
            </m:msqrt>
            <m:mo>·</m:mo>
            <m:msub>
              <m:mi>p</m:mi>
              <m:msup>
                <m:mi>f</m:mi>
                <m:mo>*</m:mo>
              </m:msup>
            </m:msub>
            <m:mrow>
              <m:mo>(</m:mo>
              <m:mi>y</m:mi>
              <m:mo>)</m:mo>
            </m:mrow>
            <m:mspace width="0.166667em"/>
            <m:mi mathvariant="normal">d</m:mi>
            <m:mi>y</m:mi>
            <m:mo>=</m:mo>
            <m:mo>∫</m:mo>
            <m:msqrt>
              <m:mrow>
                <m:msub>
                  <m:mi>p</m:mi>
                  <m:mi>f</m:mi>
                </m:msub>
                <m:mrow>
                  <m:mo>(</m:mo>
                  <m:mi>y</m:mi>
                  <m:mo>)</m:mo>
                </m:mrow>
                <m:mo>·</m:mo>
                <m:msub>
                  <m:mi>p</m:mi>
                  <m:msup>
                    <m:mi>f</m:mi>
                    <m:mo>*</m:mo>
                  </m:msup>
                </m:msub>
                <m:mrow>
                  <m:mo>(</m:mo>
                  <m:mi>y</m:mi>
                  <m:mo>)</m:mo>
                </m:mrow>
              </m:mrow>
            </m:msqrt>
            <m:mspace width="0.166667em"/>
            <m:mi mathvariant="normal">d</m:mi>
            <m:mi>y</m:mi>
          </m:mrow>
        </m:math>
      </equation>
      <para id="id2269854">and</para>
      <equation id="id2269860">
        <m:math mode="display">
          <m:mrow>
            <m:munder>
              <m:mo>∑</m:mo>
              <m:mrow>
                <m:mi>f</m:mi>
                <m:mo>∈</m:mo>
                <m:mi mathvariant="script">F</m:mi>
              </m:mrow>
            </m:munder>
            <m:msup>
              <m:mn>2</m:mn>
              <m:mrow>
                <m:mo>-</m:mo>
                <m:mi>c</m:mi>
                <m:mo>(</m:mo>
                <m:mi>f</m:mi>
                <m:mo>)</m:mo>
              </m:mrow>
            </m:msup>
            <m:mo>≤</m:mo>
            <m:mn>1</m:mn>
            <m:mo>.</m:mo>
          </m:mrow>
        </m:math>
      </equation>
      
    </statement>
</rule></section>
