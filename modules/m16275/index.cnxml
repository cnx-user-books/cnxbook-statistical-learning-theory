        </m:math>
      </equation>
      <para id="id2267133">Then can write</para>
      <equation id="id2267138">
        <m:math mode="display">
          <m:mtable displaystyle="true">
            <m:mtr>
              <m:mtd columnalign="right">
                <m:mrow>
                  <m:mi>E</m:mi>
                  <m:mfenced separators="" open="[" close="]">
                    <m:msup>
                      <m:mi>H</m:mi>
                      <m:mn>2</m:mn>
                    </m:msup>
                    <m:mfenced separators="" open="(" close=")">
                      <m:msub>
                        <m:mi>p</m:mi>
                        <m:mover accent="true">
                          <m:msub>
                            <m:mi>f</m:mi>
                            <m:mi>n</m:mi>
                          </m:msub>
                          <m:mo>^</m:mo>
                        </m:mover>
                      </m:msub>
                      <m:mo>,</m:mo>
                      <m:msub>
                        <m:mi>p</m:mi>
                        <m:msup>
                          <m:mi>f</m:mi>
                          <m:mo>*</m:mo>
                        </m:msup>
                      </m:msub>
                    </m:mfenced>
                  </m:mfenced>
                </m:mrow>
              </m:mtd>
              <m:mtd>
                <m:mo>≤</m:mo>
              </m:mtd>
              <m:mtd columnalign="left">
                <m:mrow>
                  <m:mn>2</m:mn>
                  <m:mspace width="0.166667em"/>
                  <m:mi>E</m:mi>
                  <m:mfenced separators="" open="[" close="]">
                    <m:mo form="prefix">log</m:mo>
                    <m:mfenced separators="" open="(" close=")">
                      <m:mfrac>
                        <m:mn>1</m:mn>
                        <m:mrow>
                          <m:mo>∫</m:mo>
                          <m:msqrt>
                            <m:mrow>
                              <m:msub>
                                <m:mi>p</m:mi>
                                <m:mover accent="true">
                                  <m:msub>
                                    <m:mi>f</m:mi>
                                    <m:mi>n</m:mi>
                                  </m:msub>
                                  <m:mo>^</m:mo>
                                </m:mover>
                              </m:msub>
                              <m:mrow>
                                <m:mo>(</m:mo>
                                <m:mi>y</m:mi>
                                <m:mo>)</m:mo>
                              </m:mrow>
                              <m:mo>·</m:mo>
                              <m:msub>
                                <m:mi>p</m:mi>
                                <m:msup>
                                  <m:mi>f</m:mi>
                                  <m:mo>*</m:mo>
                                </m:msup>
                              </m:msub>
                              <m:mrow>
                                <m:mo>(</m:mo>
                                <m:mi>y</m:mi>
                                <m:mo>)</m:mo>
                              </m:mrow>
                            </m:mrow>
                          </m:msqrt>
                          <m:mspace width="0.166667em"/>
                          <m:mi mathvariant="normal">d</m:mi>
                          <m:mi>y</m:mi>
                        </m:mrow>
                      </m:mfrac>
                    </m:mfenced>
                  </m:mfenced>
                </m:mrow>
              </m:mtd>
            </m:mtr>
            <m:mtr>
              <m:mtd/>
              <m:mtd>
                <m:mo>≤</m:mo>
              </m:mtd>
              <m:mtd columnalign="left">
                <m:mrow>
                  <m:mn>2</m:mn>
                  <m:mspace width="0.166667em"/>
                  <m:mi>E</m:mi>
                  <m:mfenced separators="" open="[" close="]">
                    <m:mo form="prefix">log</m:mo>
                    <m:mfenced separators="" open="(" close=")">
                      <m:mfrac>
                        <m:mrow>
                          <m:msqrt>
                            <m:mrow>
                              <m:msub>
                                <m:mi>p</m:mi>
                                <m:mover accent="true">
                                  <m:msub>
                                    <m:mi>f</m:mi>
                                    <m:mi>n</m:mi>
                                  </m:msub>
                                  <m:mo>^</m:mo>
                                </m:mover>
                              </m:msub>
                              <m:mrow>
                                <m:mo>(</m:mo>
                                <m:mi>Y</m:mi>
                                <m:mo>)</m:mo>
                              </m:mrow>
                            </m:mrow>
                          </m:msqrt>
                          <m:msup>
                            <m:mi>e</m:mi>
                            <m:mrow>
                              <m:mo>-</m:mo>
                              <m:mi>c</m:mi>
                              <m:mo>(</m:mo>
                              <m:mover accent="true">
                                <m:msub>
                                  <m:mi>f</m:mi>
                                  <m:mi>n</m:mi>
                                </m:msub>
                                <m:mo>^</m:mo>
                              </m:mover>
                              <m:mo>)</m:mo>
                              <m:mo form="prefix">log</m:mo>
                              <m:mn>2</m:mn>
                            </m:mrow>
                          </m:msup>
                        </m:mrow>
                        <m:mrow>
                          <m:msqrt>
                            <m:mrow>
                              <m:msub>
                                <m:mi>p</m:mi>
                                <m:msub>
                                  <m:mi>f</m:mi>
                                  <m:mi>n</m:mi>
                                </m:msub>
                              </m:msub>
                              <m:mrow>
                                <m:mo>(</m:mo>
                                <m:mi>Y</m:mi>
                                <m:mo>)</m:mo>
                              </m:mrow>
                            </m:mrow>
                          </m:msqrt>
                          <m:msup>
                            <m:mi>e</m:mi>
                            <m:mrow>
                              <m:mo>-</m:mo>
                              <m:mi>c</m:mi>
                              <m:mo>(</m:mo>
                              <m:msub>
                                <m:mi>f</m:mi>
                                <m:mi>n</m:mi>
                              </m:msub>
                              <m:mo>)</m:mo>
                              <m:mo form="prefix">log</m:mo>
                              <m:mn>2</m:mn>
                            </m:mrow>
                          </m:msup>
                        </m:mrow>
                      </m:mfrac>
                      <m:mo>·</m:mo>
                      <m:mfrac>
                        <m:mn>1</m:mn>
