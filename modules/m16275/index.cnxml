                    </m:mrow>
                    <m:mi>n</m:mi>
                  </m:munderover>
                  <m:msup>
                    <m:mfenced separators="" open="(" close=")">
                      <m:mover accent="true">
                        <m:msub>
                          <m:mi>f</m:mi>
                          <m:mi>n</m:mi>
                        </m:msub>
                        <m:mo>^</m:mo>
                      </m:mover>
                      <m:mrow>
                        <m:mo>(</m:mo>
                        <m:msub>
                          <m:mi>x</m:mi>
                          <m:mi>i</m:mi>
