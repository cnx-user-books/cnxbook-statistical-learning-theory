                    </m:mrow>
                    <m:mi>n</m:mi>
                  </m:munderover>
                  <m:mspace width="0.166667em"/>
                  <m:mo>-</m:mo>
                  <m:mn>2</m:mn>
                  <m:mo form="prefix">log</m:mo>
                  <m:mo>∫</m:mo>
                  <m:msqrt>
                    <m:mrow>
                      <m:msub>
                        <m:mi>p</m:mi>
                        <m:mrow>
                          <m:mover accent="true">
                            <m:msub>
                              <m:mi>f</m:mi>
                              <m:mi>n</m:mi>
                            </m:msub>
                            <m:mo>^</m:mo>
                          </m:mover>
                          <m:mrow>
                            <m:mo>(</m:mo>
                            <m:msub>
                              <m:mi>x</m:mi>
                              <m:mi>i</m:mi>
                            </m:msub>
                            <m:mo>)</m:mo>
                          </m:mrow>
                        </m:mrow>
                      </m:msub>
                      <m:mrow>
                        <m:mo>(</m:mo>
                        <m:msub>
                          <m:mi>y</m:mi>
                          <m:mi>i</m:mi>
                        </m:msub>
                        <m:mo>)</m:mo>
                      </m:mrow>
                      <m:mo>·</m:mo>
                      <m:msub>
                        <m:mi>p</m:mi>
                        <m:mrow>
                          <m:msup>
                            <m:mi>f</m:mi>
                            <m:mo>*</m:mo>
                          </m:msup>
                          <m:mrow>
                            <m:mo>(</m:mo>
                            <m:msub>
                              <m:mi>x</m:mi>
                              <m:mi>i</m:mi>
                            </m:msub>
                            <m:mo>)</m:mo>
                          </m:mrow>
                        </m:mrow>
                      </m:msub>
                      <m:mrow>
                        <m:mo>(</m:mo>
                        <m:msub>
                          <m:mi>y</m:mi>
                          <m:mi>i</m:mi>
                        </m:msub>
                        <m:mo>)</m:mo>
                      </m:mrow>
                    </m:mrow>
                  </m:msqrt>
                  <m:mspace width="0.166667em"/>
                  <m:mi mathvariant="normal">d</m:mi>
                  <m:msub>
                    <m:mi>y</m:mi>
