                    <m:mi>c</m:mi>
                    <m:mo>(</m:mo>
                    <m:mover accent="true">
                      <m:msub>
                        <m:mi>f</m:mi>
                        <m:mi>n</m:mi>
                      </m:msub>
                      <m:mo>^</m:mo>
                    </m:mover>
                    <m:mo>)</m:mo>
