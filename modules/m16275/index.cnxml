              </m:mrow>
              <m:mrow>
                <m:msqrt>
                  <m:mrow>
                    <m:msub>
                      <m:mi>p</m:mi>
                      <m:msub>
                        <m:mi>f</m:mi>
                        <m:mi>n</m:mi>
                      </m:msub>
                    </m:msub>
                    <m:mrow>
                      <m:mo>(</m:mo>
                      <m:mi>Y</m:mi>
                      <m:mo>)</m:mo>
                    </m:mrow>
                  </m:mrow>
                </m:msqrt>
                <m:msup>
                  <m:mi>e</m:mi>
                  <m:mrow>
                    <m:mo>-</m:mo>
                    <m:mi>c</m:mi>
                    <m:mo>(</m:mo>
                    <m:msub>
                      <m:mi>f</m:mi>
                      <m:mi>n</m:mi>
                    </m:msub>
