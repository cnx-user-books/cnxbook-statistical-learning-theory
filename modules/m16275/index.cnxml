                          <m:mo>∫</m:mo>
                          <m:msqrt>
                            <m:mrow>
                              <m:msub>
                                <m:mi>p</m:mi>
                                <m:mover accent="true">
                                  <m:msub>
                                    <m:mi>f</m:mi>
                                    <m:mi>n</m:mi>
                                  </m:msub>
                                  <m:mo>^</m:mo>
                                </m:mover>
