      <para id="id2266985">we can see that</para>
      <equation id="id2266991">
        <m:math mode="display">
          <m:mrow>
            <m:mfrac>
              <m:mrow>
                <m:msqrt>
                  <m:mrow>
                    <m:msub>
                      <m:mi>p</m:mi>
                      <m:mover accent="true">
                        <m:msub>
                          <m:mi>f</m:mi>
                          <m:mi>n</m:mi>
                        </m:msub>
                        <m:mo>^</m:mo>
                      </m:mover>
                    </m:msub>
                    <m:mrow>
                      <m:mo>(</m:mo>
                      <m:mi>Y</m:mi>
                      <m:mo>)</m:mo>
                    </m:mrow>
                  </m:mrow>
                </m:msqrt>
                <m:msup>
                  <m:mi>e</m:mi>
                  <m:mrow>
