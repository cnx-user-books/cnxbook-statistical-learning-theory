                    </m:mrow>
                    <m:mi>n</m:mi>
                  </m:munderover>
                  <m:mfrac>
                    <m:msup>
                      <m:mfenced separators="" open="(" close=")">
                        <m:mi>f</m:mi>
                        <m:mrow>
                          <m:mo>(</m:mo>
                          <m:msub>
                            <m:mi>x</m:mi>
                            <m:mi>i</m:mi>
                          </m:msub>
                          <m:mo>)</m:mo>
                        </m:mrow>
                        <m:mo>-</m:mo>
                        <m:msup>
                          <m:mi>f</m:mi>
                          <m:mo>*</m:mo>
                        </m:msup>
                        <m:mrow>
                          <m:mo>(</m:mo>
                          <m:msub>
                            <m:mi>x</m:mi>
                            <m:mi>i</m:mi>
                          </m:msub>
                          <m:mo>)</m:mo>
                        </m:mrow>
                      </m:mfenced>
                      <m:mn>2</m:mn>
                    </m:msup>
                    <m:mrow>
                      <m:mn>2</m:mn>
                      <m:msup>
                        <m:mi>σ</m:mi>
