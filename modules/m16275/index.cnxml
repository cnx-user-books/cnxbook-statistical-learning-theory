                                </m:msub>
                                <m:mo>^</m:mo>
                              </m:mover>
                            </m:msub>
                            <m:mrow>
                              <m:mo>(</m:mo>
                              <m:mi>Y</m:mi>
                              <m:mo>)</m:mo>
                            </m:mrow>
                          </m:mrow>
                        </m:msqrt>
                        <m:msqrt>
                          <m:mrow>
                            <m:msub>
                              <m:mi>p</m:mi>
                              <m:msup>
                                <m:mi>f</m:mi>
                                <m:mo>*</m:mo>
