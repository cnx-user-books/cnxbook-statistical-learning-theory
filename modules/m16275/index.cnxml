                  <m:mfenced separators="" open="{" close="}">
                    <m:mo form="prefix">log</m:mo>
                    <m:mfenced separators="" open="(" close=")">
                      <m:msqrt>
                        <m:mrow>
                          <m:msub>
                            <m:mi>p</m:mi>
                            <m:mi>f</m:mi>
                          </m:msub>
                          <m:mrow>
                            <m:mo>(</m:mo>
