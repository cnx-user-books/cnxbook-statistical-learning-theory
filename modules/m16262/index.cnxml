                    </m:mfrac>
                    <m:mo form="prefix">log</m:mo>
                    <m:mi>n</m:mi>
                  </m:mrow>
                  <m:mrow>
                    <m:mn>2</m:mn>
                    <m:mi>n</m:mi>
                  </m:mrow>
