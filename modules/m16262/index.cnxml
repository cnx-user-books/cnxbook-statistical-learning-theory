            </m:munder>
            <m:mfenced separators="" open="(" close=")">
              <m:munder>
                <m:mo movablelimits="true" form="prefix">min</m:mo>
                <m:mrow>
                  <m:mi>f</m:mi>
                  <m:mo>∈</m:mo>
                  <m:mi>d</m:mi>
                  <m:mi>y</m:mi>
                  <m:mi>a</m:mi>
                  <m:mi>d</m:mi>
                  <m:mi>i</m:mi>
                  <m:mi>c</m:mi>
                  <m:mspace width="4pt"/>
                  <m:mi>k</m:mi>
                  <m:mspace width="4pt"/>
                  <m:mi>l</m:mi>
                  <m:mi>e</m:mi>
                  <m:mi>a</m:mi>
                  <m:mi>f</m:mi>
                  <m:mspace width="4pt"/>
                  <m:mi>t</m:mi>
                  <m:mi>r</m:mi>
                  <m:mi>e</m:mi>
                  <m:mi>e</m:mi>
                  <m:mi>s</m:mi>
                </m:mrow>
              </m:munder>
              <m:mi>R</m:mi>
              <m:mrow>
                <m:mo>(</m:mo>
                <m:mi>f</m:mi>
                <m:mo>)</m:mo>
              </m:mrow>
