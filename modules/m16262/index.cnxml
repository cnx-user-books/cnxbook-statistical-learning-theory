            </m:msubsup>
            <m:mo>=</m:mo>
            <m:mo form="prefix">arg</m:mo>
            <m:munder>
              <m:mo movablelimits="true" form="prefix">min</m:mo>
              <m:mrow>
                <m:mi>f</m:mi>
                <m:mo>∈</m:mo>
                <m:msub>
                  <m:mi mathvariant="script">F</m:mi>
                  <m:mi>k</m:mi>
                </m:msub>
              </m:mrow>
            </m:munder>
            <m:msub>
              <m:mover accent="true">
