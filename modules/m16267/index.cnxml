                  <m:mo>[</m:mo>
                  <m:msub>
                    <m:mi>T</m:mi>
                    <m:mn>1</m:mn>
                  </m:msub>
                  <m:mo>]</m:mo>
                </m:mrow>
              </m:mtd>
              <m:mtd>
                <m:mo>=</m:mo>
              </m:mtd>
              <m:mtd columnalign="left">
                <m:mrow>
                  <m:mn>4</m:mn>
                  <m:mi>E</m:mi>
                  <m:mo>[</m:mo>
                  <m:msup>
                    <m:mfenced separators="" open="(" close=")">
                      <m:mrow>
                        <m:mo>(</m:mo>
                        <m:msub>
                          <m:mi>Y</m:mi>
                          <m:mi>i</m:mi>
                        </m:msub>
                        <m:mo>-</m:mo>
                        <m:msup>
                          <m:mi>f</m:mi>
                          <m:mo>*</m:mo>
                        </m:msup>
                        <m:mrow>
                          <m:mo>(</m:mo>
                          <m:msub>
                            <m:mi>X</m:mi>
                            <m:mi>i</m:mi>
                          </m:msub>
                          <m:mo>)</m:mo>
                        </m:mrow>
                        <m:mo>)</m:mo>
                      </m:mrow>
                      <m:mrow>
                        <m:mo>(</m:mo>
                        <m:mi>f</m:mi>
                        <m:mrow>
                          <m:mo>(</m:mo>
                          <m:msub>
                            <m:mi>X</m:mi>
                            <m:mi>i</m:mi>
                          </m:msub>
                          <m:mo>)</m:mo>
                        </m:mrow>
                        <m:mo>-</m:mo>
                        <m:msup>
                          <m:mi>f</m:mi>
                          <m:mo>*</m:mo>
                        </m:msup>
                        <m:mrow>
                          <m:mo>(</m:mo>
                          <m:msub>
                            <m:mi>X</m:mi>
                            <m:mi>i</m:mi>
                          </m:msub>
                          <m:mo>)</m:mo>
                        </m:mrow>
                        <m:mo>)</m:mo>
                      </m:mrow>
                    </m:mfenced>
                    <m:mn>2</m:mn>
                  </m:msup>
                  <m:mo>]</m:mo>
                </m:mrow>
              </m:mtd>
            </m:mtr>
            <m:mtr>
              <m:mtd/>
              <m:mtd>
                <m:mo>=</m:mo>
              </m:mtd>
              <m:mtd columnalign="left">
                <m:mrow>
                  <m:mn>4</m:mn>
                  <m:mi>E</m:mi>
                  <m:mo>[</m:mo>
