                  </m:mrow>
                  <m:mi>m</m:mi>
                </m:munderover>
                <m:msub>
                  <m:mo>∫</m:mo>
                  <m:msub>
                    <m:mi>I</m:mi>
                    <m:mrow>
                      <m:mi>j</m:mi>
