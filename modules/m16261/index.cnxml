          </m:mtr>
          <m:mtr>
            <m:mtd/>
            <m:mtd>
              <m:mo>≤</m:mo>
            </m:mtd>
            <m:mtd columnalign="left">
              <m:mrow>
                <m:munderover>
                  <m:mo>∑</m:mo>
                  <m:mrow>
                    <m:mi>j</m:mi>
                    <m:mo>=</m:mo>
                    <m:mn>1</m:mn>
                  </m:mrow>
                  <m:mi>m</m:mi>
                </m:munderover>
                <m:msub>
                  <m:mo>∫</m:mo>
                  <m:msub>
                    <m:mi>I</m:mi>
                    <m:mrow>
                      <m:mi>j</m:mi>
                      <m:mo>,</m:mo>
                      <m:mi>m</m:mi>
                    </m:mrow>
                  </m:msub>
                </m:msub>
                <m:msup>
                  <m:mfenced separators="" open="(" close=")">
                    <m:mfrac>
                      <m:mn>1</m:mn>
                      <m:mi>k</m:mi>
                    </m:mfrac>
                    <m:munder>
                      <m:mo>∑</m:mo>
                      <m:mrow>
                        <m:mi>i</m:mi>
                        <m:mo>:</m:mo>
                        <m:mfrac>
                          <m:mi>i</m:mi>
                          <m:mi>n</m:mi>
                        </m:mfrac>
                        <m:mo>∈</m:mo>
                        <m:msub>
                          <m:mi>I</m:mi>
                          <m:mrow>
                            <m:mi>j</m:mi>
                            <m:mo>,</m:mo>
