                          </m:mrow>
                        </m:msub>
                      </m:mrow>
                    </m:munder>
                    <m:mfrac>
                      <m:mi>L</m:mi>
                      <m:mi>m</m:mi>
                    </m:mfrac>
                  </m:mfenced>
                  <m:mn>2</m:mn>
                </m:msup>
                <m:mi>d</m:mi>
                <m:mi>t</m:mi>
              </m:mrow>
            </m:mtd>
          </m:mtr>
          <m:mtr>
            <m:mtd/>
            <m:mtd>
              <m:mo>=</m:mo>
            </m:mtd>
            <m:mtd columnalign="left">
              <m:mrow>
                <m:munderover>
                  <m:mo>∑</m:mo>
                  <m:mrow>
                    <m:mi>j</m:mi>
                    <m:mo>=</m:mo>
                    <m:mn>1</m:mn>
                  </m:mrow>
                  <m:mi>m</m:mi>
                </m:munderover>
                <m:msub>
                  <m:mo>∫</m:mo>
                  <m:msub>
                    <m:mi>I</m:mi>
                    <m:mrow>
                      <m:mi>j</m:mi>
                      <m:mo>,</m:mo>
                      <m:mi>m</m:mi>
                    </m:mrow>
                  </m:msub>
                </m:msub>
                <m:msup>
                  <m:mfenced separators="" open="(" close=")">
                    <m:mfrac>
                      <m:mi>L</m:mi>
                      <m:mi>m</m:mi>
                    </m:mfrac>
                  </m:mfenced>
                  <m:mn>2</m:mn>
                </m:msup>
                <m:mi>d</m:mi>
