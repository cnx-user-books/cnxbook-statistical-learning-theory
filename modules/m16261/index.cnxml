                    </m:mfrac>
                    <m:munder>
                      <m:mo>∑</m:mo>
                      <m:mrow>
                        <m:mi>i</m:mi>
                        <m:mo>:</m:mo>
                        <m:mfrac>
                          <m:mi>i</m:mi>
                          <m:mi>n</m:mi>
                        </m:mfrac>
                        <m:mo>∈</m:mo>
                        <m:msub>
                          <m:mi>I</m:mi>
                          <m:mrow>
                            <m:mi>j</m:mi>
                            <m:mo>,</m:mo>
                            <m:mi>m</m:mi>
                          </m:mrow>
                        </m:msub>
                      </m:mrow>
                    </m:munder>
                    <m:mfenced separators="" open="|" close="|">
                      <m:msup>
                        <m:mi>f</m:mi>
                        <m:mo>*</m:mo>
                      </m:msup>
                      <m:mrow>
                        <m:mo>(</m:mo>
                        <m:mi>t</m:mi>
                        <m:mo>)</m:mo>
                      </m:mrow>
                      <m:mo>-</m:mo>
                      <m:msup>
                        <m:mi>f</m:mi>
                        <m:mo>*</m:mo>
                      </m:msup>
                      <m:mfenced separators="" open="(" close=")">
                        <m:mfrac>
