                      <m:mo>,</m:mo>
                      <m:mi>m</m:mi>
                    </m:mrow>
                  </m:msub>
                </m:msub>
                <m:msup>
                  <m:mfenced separators="" open="(" close=")">
                    <m:mfrac>
