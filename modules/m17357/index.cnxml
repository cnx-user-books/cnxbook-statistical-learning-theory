                            <m:mrow>
                              <m:mo>(</m:mo>
                              <m:msub>
                                <m:mi>X</m:mi>
                                <m:mi>i</m:mi>
                              </m:msub>
                              <m:mo>,</m:mo>
                              <m:msub>
                                <m:mi>Y</m:mi>
                                <m:mi>i</m:mi>
                              </m:msub>
