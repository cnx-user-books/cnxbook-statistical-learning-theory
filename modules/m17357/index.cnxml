                </m:mtr>
                <m:mtr>
                  <m:mtd/>
                  <m:mtd>
                    <m:mo>=</m:mo>
                  </m:mtd>
                  <m:mtd columnalign="left">
                    <m:mrow>
                      <m:mo>∫</m:mo>
                      <m:mo movablelimits="true" form="prefix">min</m:mo>
                      <m:mrow>
                        <m:mo>(</m:mo>
                        <m:msub>
