                        </m:msub>
                      </m:mfenced>
                      <m:mspace width="56.9055pt"/>
                      <m:mtext>by</m:mtext>
                      <m:mspace width="4.pt"/>
                      <m:mtext>Jensen's</m:mtext>
                      <m:mspace width="4.pt"/>
                      <m:mtext>inequality</m:mtext>
