                                  <m:mn>1</m:mn>
                                </m:msub>
                                <m:mo>,</m:mo>
                                <m:msub>
                                  <m:mi>Y</m:mi>
                                  <m:mn>1</m:mn>
                                </m:msub>
                              </m:mrow>
                              <m:mrow>
                                <m:mo>(</m:mo>
                                <m:mn>0</m:mn>
                                <m:mo>)</m:mo>
