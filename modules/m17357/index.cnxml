                <m:mtr>
                  <m:mtd/>
                  <m:mtd>
                    <m:mo>=</m:mo>
                  </m:mtd>
                  <m:mtd columnalign="left">
                    <m:mrow>
                      <m:mi>n</m:mi>
                      <m:msub>
                        <m:mi mathvariant="double-struck">E</m:mi>
                        <m:mn>1</m:mn>
                      </m:msub>
                      <m:mfenced separators="" open="[" close="]">
                        <m:mo form="prefix">log</m:mo>
                        <m:mfrac>
                          <m:mrow>
                            <m:msubsup>
                              <m:mi>p</m:mi>
                              <m:mrow>
                                <m:mi>Y</m:mi>
