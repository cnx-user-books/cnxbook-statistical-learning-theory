                      <m:msub>
                        <m:mi mathvariant="double-struck">E</m:mi>
                        <m:mn>1</m:mn>
                      </m:msub>
                      <m:mfenced separators="" open="[" close="]">
                        <m:mo form="prefix">log</m:mo>
                        <m:mfrac>
                          <m:mrow>
                            <m:msubsup>
                              <m:mi>p</m:mi>
                              <m:mrow>
                                <m:msub>
                                  <m:mi>X</m:mi>
                                  <m:mi>i</m:mi>
                                </m:msub>
