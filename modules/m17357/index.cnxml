          <para id="id2261999">For the second inequality,</para>
          <equation id="id2262005">
            <m:math mode="display">
              <m:mtable displaystyle="true">
                <m:mtr>
                  <m:mtd columnalign="right">
                    <m:mrow>
                      <m:msup>
                        <m:mi>A</m:mi>
                        <m:mn>2</m:mn>
                      </m:msup>
                      <m:mrow>
                        <m:mo>(</m:mo>
                        <m:msub>
                          <m:mi>P</m:mi>
                          <m:mn>0</m:mn>
