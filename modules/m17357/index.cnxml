                          </m:mrow>
                        </m:msqrt>
                        <m:msqrt>
                          <m:mrow>
                            <m:mo movablelimits="true" form="prefix">max</m:mo>
                            <m:mo>(</m:mo>
                            <m:msub>
                              <m:mi>p</m:mi>
                              <m:mn>0</m:mn>
                            </m:msub>
                            <m:mo>,</m:mo>
                            <m:msub>
                              <m:mi>p</m:mi>
                              <m:mn>1</m:mn>
                            </m:msub>
                            <m:mo>)</m:mo>
                          </m:mrow>
                        </m:msqrt>
                      </m:mfenced>
                      <m:mn>2</m:mn>
